{% set classes = [
    'view',
    'view-' ~ id|clean_class,
    'view-id-' ~ id,
    'view-display-id-' ~ display_id,
    dom_id ? 'js-view-dom-id-' ~ dom_id,
  ]
%}
<div{{ attributes.addClass(classes) }}>

{% if rows %}
    {{ rows }}
{% elseif empty %}
    {{ empty }}
{% endif %}

{% if pager %}
    {{ pager }}
{% endif %}

</div>